pipeline
{
    agent any
    stages
    {
        stage('fetch')
        {
            steps{
                  git 'https://github.com/pulkitpan/task.git'
            }
        }
        
        stage('Test')
        {
            steps{
            echo "Testing..Phase"
            }
        }
        stage('Deploy')
        {
            steps{
            echo 'Deploying..Phase'
            }
        }
    }
}
